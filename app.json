{
  "expo": {
    "name": "StudyFlow",
    "slug": "Study-Flow",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#4F46E5"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tdat.studyflow",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.tdat.studyflow",
      "permissions": ["android.permission.RECORD_AUDIO"]
    },
    "web": {
      "bundler": "metro"
    },
    "platforms": ["ios", "android", "web"],
    "extra": {
      "eas": {
        "projectId": "f14401f5-260e-4e86-9826-63e2c1aaa039"
      }
    }
  }
}
